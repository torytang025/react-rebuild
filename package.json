{
	"name": "react-rebuild",
	"version": "1.0.0",
	"description": "",
	"keywords": [],
	"license": "ISC",
	"author": "",
	"main": "index.js",
	"scripts": {
		"build": "pnpm clean:dist && pnpm -r --filter='./packages/**' run build",
		"clean:dist": "rimraf --glob '**/dist'",
		"clean:nm": "rimraf node_modules packages/*/node_modules",
		"demo:react": "pnpm -r --filter='./demos/react' run dev",
		"dev": "NODE_ENV=development pnpm -r --filter='./packages/**' dev",
		"lint": "eslint --ext .js,.ts,.jsx,.tsx --fix --quiet ./packages",
		"prepare": "husky"
	},
	"lint-staged": {
		"*.{js,ts}": [
			"eslint --cache --fix"
		]
	},
	"dependencies": {
		"scheduler": "^0.23.2"
	},
	"devDependencies": {
		"@commitlint/cli": "^19.3.0",
		"@commitlint/config-conventional": "^19.2.2",
		"@types/node": "^20.12.12",
		"@types/scheduler": "^0.23.0",
		"@typescript-eslint/eslint-plugin": "^7.9.0",
		"@typescript-eslint/parser": "^7.9.0",
		"commitlint": "^19.3.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-import": "^2.29.1",
		"eslint-plugin-prettier": "^5.1.3",
		"eslint-plugin-simple-import-sort": "^12.1.0",
		"husky": "^9.0.11",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"prettier-plugin-packagejson": "^2.5.0",
		"rimraf": "^5.0.7",
		"typescript": "^5.4.5",
		"vite": "^5.2.11",
		"vite-plugin-dts": "^3.9.1",
		"vite-tsconfig-paths": "^4.3.2"
	}
}
